use UTbot as knowledge.

% Make a best effort attempt to go to the Destination.
define navigate(Destination) with
	pre{ not(navigation(navigating,_)) }
	post{ true }
	
% Calls the respawn action on the bot
define respawn with
	pre{ true }
	post{ true }
	
% Deploys the deployable we have currently equipped
define deploy with
	pre{ true }
	post{ true }
	
% Stops the navigation and clears the destination
define stop with
	pre{ true }
	post{ true }
	
% This action is used to define what/where to shoot.
define shoot(ID) with
	pre{ true }
	post{ true }